
@{
    ViewBag.Title = "System_Access";
}

<!DOCTYPE html>
<html>
<head>
    <link href="~/CSS/style.css" rel="stylesheet" type="text/css" media="screen" />
    <meta name="viewport" content="width=device-width" />
</head>

<body>
    <br />
    <div style="text-align: center;">
        <img src="~/Portal_Assest/QTC-logo.png" style="margin: 5px;" />
    </div>


    <div id="mainwrapper">
        <div id="box-3" class="box">
            <img id="image-3" src="~/Portal_Assest/System Access/Guest Wireless Access Request.png" style="width:265px;height:167px;" />
            <span class="caption fade-caption">
                <p>
                    To request Guest wireless
                    access (Diamond Bar, San
                    Antonio and Philadelphia
                    AO’s only)
                </p>
            </span>
        </div>


        <div id="box-3" class="box">
            <img id="image-3" src="~/Portal_Assest/System Access/Mobile Device Requests.png" style="width:265px;height:167px;" />
            <span class="caption fade-caption">
                <p>
                    To request a QTC-issued
                    mobile device
                    (mobile-email enabled cell
                    phone, Aircard or MiFi
                    device)
                </p>
            </span>
        </div>

        <div id="box-3" class="box">
            <img id="image-3" src="~/Portal_Assest/System Access/USB Storage Access Requests.png" style="width:265px;height:167px;" />
            <span class="caption fade-caption">
                <p>
                    To request access for
                    external media storage
                    access (Digital camera, USB
                    Flash drive)
                </p>
            </span>
        </div>

        <div id="box-3" class="box">
            <img id="image-3" src="~/Portal_Assest/System Access/VPN (Remote Access) Request.png" style="width:265px;height:167px;" />
            <a href="~/Portal/VPNfaq">
                <span class="caption fade-caption">
                    <p>
                        For access to QTC’s Remote
                        Access (VPN) system
                    </p>
                </span>
            </a>
        </div>

        <div id="box-3" class="box">
            <img id="image-3" src="~/Portal_Assest/System Access/WebEx Account Request.png" style="width:265px;height:167px;" />
            <br />
            <span class="caption fade-caption">
                <p>
                    For obtaining a QTC-issue
                    Cisco WebEx account
                </p>
            </span>
        </div>

    </div>

    <div style="text-align: center;">
        <a href="~/Portal/Index/"><button type="button" class="btn btn-secondary">Back</button></a>
    </div>
</body>

</html>

